  visibility = ["//visibility:public"],
  deps = [
    ":proto_lib",
  ],
  name = "cc_proto",
)

java_library(
  visibility = ["//visibility:public"],
  deps = [
    ":generate_java_descriptor_proto",
  ],
  name = "java_proto",
  srcs = glob([
    "repository/java/src/main/java/com/google/protobuf/*.java"
  ]),
)

genrule(
  name = "generate_java_descriptor_proto",
  srcs = [
    ":protoc",
    "repository/src/google/protobuf/descriptor.proto",
  ],
  cmd = "$(location :protoc) --java_out=$(GENDIR)/third_party/protobuf/java -Ithird_party/protobuf/repository/src $(location repository/src/google/protobuf/descriptor.proto)",
  outs = [ "java/com/google/protobuf/DescriptorProtos.java" ],
)

genrule(
  name = "generate_py_descriptor_proto",
  srcs = [
    ":protoc",
    "repository/src/google/protobuf/descriptor.proto",
    "repository/src/google/protobuf/compiler/plugin.proto",
  ],
  cmd = "$(location :protoc) --python_out=$(GENDIR)/third_party/protobuf/python -Ithird_party/protobuf/repository/src $(location repository/src/google/protobuf/descriptor.proto) $(location repository/src/google/protobuf/compiler/plugin.proto)",
  outs = [
    "python/google/protobuf/descriptor_pb2.py",
    "python/google/protobuf/compiler/plugin_pb2.py",
  ],
)

COMMON_COPTS = [
  "-DHAVE_CONFIG_H",
  "-pthread",
  "-Wall",
  "-Wwrite-strings",
  "-Woverloaded-virtual",
  "-Wno-sign-compare",
  "-Wno-error=unused-function",
  "-Ithird_party/protobuf/build/k8",
  "-Ithird_party/protobuf/build/k8/src",
]

